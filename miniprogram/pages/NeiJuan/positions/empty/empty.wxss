.container{
    position:fixed;
    display:flex;
    flex-direction: column;
    justify-content: space-around;
    width:100%;
    height:90%;
}

.main{
    width:100%;
    display:flex;
    flex-direction: column;
    align-items: center;
}

.title{
    margin:20px;
    font-size:x-large;
    font-weight:bold;
    color:red;
}